[git-up "fetch"]
	prune = true
[git-up "rebase"]
	log-hook = "echo \"changes on $1:\"; git ls --no-merges $1..$2"
[user]
	name = Blake ETEME
	email = blake.eteme@gmail.com
[core]
	editor = emacs
	sharedRepository = umask
	autocrlf = input
    	filemode = false
[color]
	ui = auto
[github]
	user = horrorin
[push]
	default = tracking
[branch]
	autosetuprebase = always
[merge]
	conflictStyle = diff3
[mergetool]
	keepBackup = false
	keepTemporaries = false
	prompt = false
[credential]
	helper = cache --timeout=3600
[alias]
	alias = "!sh -c '[ $# = 2 ] && git config --global alias.\"$1\" \"$2\" && exit 0 || echo \"usage: git alias <new alias> <original command>\" >&2 && exit 1' -"
	aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
	a = add --all
    	amend = commit --amend
    	ap = add -p
    	assume = update-index --assume-unchanged
    	assumeall = "!git st -s | awk {'print $2'} | xargs git assume"
    	assumed = "!git ls-files -v | grep ^h | cut -c 3-"
    	br = branch
    	c  = commit
	cm = commit -m
    	co = checkout
    	cp = cherry-pick
	cr = !sh -c \". $HOME/bin/git-create.sh\"
    	d  = diff
    	diffr  = "!f() { git diff "$1"^.."$1"; }; f"
    	dl = "!git ll -1"
    	dlc = diff --cached HEAD^
    	dp = diff -p
    	dr  = "!f() { git diff "$1"^.."$1"; }; f"
    	ds = diff --staged
    	filelog = log -u
    	fl = log -u
    	la = "!git config -l | grep alias | cut -c 7-"
    	lc  = "!f() { git ll "$1"^.."$1"; }; f"
    	ld = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
    	lds = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
    	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    	ll = log --pretty=format:'%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --numstat --no-merges
    	lnc = log --pretty=format:"%h\\ %s\\ [%cn]"
    	lo = "log --pretty=\"%h %an %ar %s\""
    	ls = log --pretty=format:'%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --no-merges
    	mt = mergetool
    	ours = "!f() { git co --ours $@ && git add $@; }; f"
    	pod = push origin develop
    	re = rebase
    	rea = rebase --abort
    	rec = rebase --continue
    	res = rebase --skip
    	rh = reset HEAD
    	sa = stash apply
    	sl = stash list
    	ss = stash save
	st = status -sb
    	theirs = "!f() { git co --theirs $@ && git add $@; }; f"
    	unassume = update-index --no-assume-unchanged
    	unassumeall = "!git assumed | xargs git update-index --no-assume-unchanged"
